<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory>
        <SwitchPreference
            android:key="roaming"
            android:summary="로밍 시 데이터 서비스에 연결"
            android:title="데이터 미리보기" />
    </PreferenceCategory>
    <ListPreference
        android:entries="@array/array_network"
        android:entryValues="@array/array_network"
        android:key="network"
        android:summary="2G"
        android:title="기본 네트워크 설정" />
    <PreferenceCategory>
        <SwitchPreference
            android:key="lte_mode"
            android:summary="LTE 서비스를 사용하여 음성 및 기타 통신을 개선할 수 있습니다.(권장)"
            android:title="향상된 4G LTE모드" />
    </PreferenceCategory>
    <Preference
        android:key="apn"
        android:title="APN" />
    <PreferenceScreen
        android:key="manager"
        android:summary="네트워크 운영자 선택"
        android:title="네트워크 운영자">
        <intent
            android:targetClass="com.example.test9.SubActivity"
            android:targetPackage="com.example.test9" />
    </PreferenceScreen>
</PreferenceScreen>